<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="beis-ui-cache" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="true" monitoring="autodetect"
         dynamicConfig="true">

    <defaultCache name="defaultCache"
            maxEntriesLocalHeap="0"
            eternal="false"
            timeToIdleSeconds="120"
            timeToLiveSeconds="120"
            memoryStoreEvictionPolicy="LRU"
            statistics="false">
    </defaultCache>

    <cache name="defaultCache"
            maxEntriesLocalHeap="0"
            eternal="false"
            timeToIdleSeconds="120"
            timeToLiveSeconds="120"
            memoryStoreEvictionPolicy="LRU"
            statistics="false">
    </cache>
        
    <cache name="viewprofiles"
           maxEntriesLocalHeap="1000"
           eternal="false"
           timeToIdleSeconds="20"
           timeToLiveSeconds="120"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>

    <cache name="questionnaires"
           maxEntriesLocalHeap="10"
           eternal="false"
           timeToIdleSeconds="20"
           timeToLiveSeconds="120"
           memoryStoreEvictionPolicy="LRU"
           transactionalMode="off">
    </cache>
    
    <cache name="javascripts"
           maxEntriesLocalHeap="5"
           eternal="false"
           timeToIdleSeconds="60"
           timeToLiveSeconds="60"
           memoryStoreEvictionPolicy="LRU"
           transactionalMode="off">
    </cache>
    
    <cache name="jsregex"
           maxEntriesLocalHeap="20"
           eternal="false"
           timeToIdleSeconds="86400"
           timeToLiveSeconds="86400"
           memoryStoreEvictionPolicy="LRU"
           transactionalMode="off">
    </cache>
    
    <cache name="augmentationAnnotation"
           maxEntriesLocalHeap="1000"
           eternal="false"
           timeToIdleSeconds="86400"
           timeToLiveSeconds="86400"
           memoryStoreEvictionPolicy="LRU"
           transactionalMode="off">
    </cache>
    
    <cache name="introspectionAnnotation"
           maxEntriesLocalHeap="10"
           eternal="false"
           timeToIdleSeconds="86400"
           timeToLiveSeconds="86400"
           memoryStoreEvictionPolicy="LRU"
           transactionalMode="off">
    </cache>

    <cache name="cvlists"
           maxEntriesLocalHeap="10"
           eternal="false"
           timeToIdleSeconds="120"
           timeToLiveSeconds="120"
           memoryStoreEvictionPolicy="LRU"
           transactionalMode="off">
    </cache>    

</ehcache>